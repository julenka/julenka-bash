# Directories, etc
alias l='ls -G'
alias ..='cd ..'
alias ...='cd ../..'
alias pd='pushd '
alias pp='popd'

# Misc
alias grep='grep --color=auto'
alias h='history | cut -c 8-'
alias imgdim='sips -g pixelHeight -g pixelWidth $1'

# git
alias gt='git status'
alias gb='git branch'
alias gp='git add -p'

# other shortcuts
alias d='cd ~/devel/'
